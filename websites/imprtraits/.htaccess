RewriteOptions inherit
RewriteEngine on

RewriteRule ^order[$/]$ order/order.php [nocase]
RewriteRule ^about[$/]$ about/about.html [nocase]
RewriteRule ^gallery[$/]$ gallery/gallery.html [nocase]
RewriteRule ^logIn[$/]$ account/logInPage.php [nocase]
RewriteRule ^createAccount[$/]$ account/createAccountPage.php [nocase]
RewriteRule ^forgotPassword[$/]$ account/forgotPassword/page.php [nocase]
RewriteRule ^forgotPassword/check[$/]$ account/forgotPassword/process.php [nocase]
RewriteRule ^forgotPassword/newPassword[$/]$ account/forgotPassword/codeCheck.php [nocase]
RewriteRule ^contactMe[$/]$ about/contactMe.php [nocase]

